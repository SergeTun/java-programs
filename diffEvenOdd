class Main {
  public static void main(String[] args) {

    int[] array = {4, 9, 6, 10, 2};
    int i;
    int even = 0;
    int odd = 0;
    for (i = 0; i < array.length; i++) {
      if (array[i] % 2 == 0) {
        even++;
      }
      else {
        odd++;
      }
      
    }
    System.out.println(even - odd);
    }
  }
